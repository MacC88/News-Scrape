<div class="secondary">
<div>
    <h1 class="card z-depth-3 center-align">Click the scrape button on the top right for the latest article.</h1>
    <div class="row">
      <div class="col s3"></div>
    </div>
</div>

<div class="row">
  <div class="mx-auto">
  <div class="col s2"></div>
  <div class ="col s8 left-align">
    {{#each article}}
      <div class="card-panel">
        <h4>{{this.title}}</h4>
        <a href = "/readArticle/{{this._id}}" class="btn btn-dark" id="btnSignUp">Read Article</a>
      </div>
    {{/each}}
  </div>
  </div>
</div>
</div>